{
    "name": "react-spa-sample",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "parcel src/index.html --open --port 5000",
        "build": "parcel build src/index.html",
        "clean": "rm -rf dist",
        "test": "tsc --noEmit && jest --coverage",
        "lint": "tslint --project tsconfig.json --format stylish"
