    "bugs": {
        "url": "https://github.com/jdcwright/react-spa-sample/issues"
    },
    "homepage": "https://github.com/jdcwright/react-spa-sample#readme",
    "devDependencies": {
        "@types/bootstrap": "^4.3.1",
        "@types/jest": "^24.0.18",
        "@types/js-cookie": "^2.2.4",
        "@types/lodash": "^4.14.138",
        "@types/node": "^12.7.5",
        "@types/react": "^16.9.13",
        "@types/react-dom": "^16.9.0",
        "@types/react-redux": "^7.1.5",
        "@types/react-router-dom": "^4.3.5",
        "@types/styled-components": "^4.1.19",
        "jest": "^24.9.0",
