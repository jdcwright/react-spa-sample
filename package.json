    "jest": {
        "preset": "ts-jest",
        "transform": {
            "^.+\\.(t|j)sx?$": "ts-jest"
        },
        "collectCoverageFrom": [
            "<rootDir>/frontend/src/**/*{ts|tsx}"
        ],
        "testRegex": "/__tests__/.*\\.(ts|tsx)$"
    },
    "repository": {
        "type": "git",
        "url": "git+https://jdcwright@github.com/jdcwright/react-spa-sample.git"
    },
    "author": "",
