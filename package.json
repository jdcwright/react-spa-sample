        "lodash": "^4.17.15",
        "react": "^16.12.0",
        "react-bootstrap": "^1.0.0-beta.16",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.14",
        "react-redux": "^7.1.3",
        "react-router": "^5.0.1",
